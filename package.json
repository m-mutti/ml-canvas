{
  "name": "ml-canvas",
  "version": "0.2.0",
  "description": "A Vue.js canvas component designed for machine learning annotation tasks and AI tool development",
  "type": "module",
  "main": "./dist/ml-canvas.umd.js",
  "module": "./dist/ml-canvas.es.js",
  "exports": {
    ".": {
      "import": "./dist/ml-canvas.es.js",
      "require": "./dist/ml-canvas.umd.js"
    },
    "./style.css": "./dist/ml-canvas.css"
  },
  "files": [
    "dist",
    "src/components/MLCanvas.vue",
    "README.md",
    "CONTRIBUTING.md"
  ],
  "keywords": [
    "vue",
    "canvas",
    "machine-learning",
    "annotation",
    "ml",
    "ai",
    "computer-vision",
    "object-detection",
    "segmentation",
    "drawing",
    "labeling"
  ],
  "author": "Muhammad Mutti <mutti.rehman1122@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/m-mutti/ml-canvas#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/m-mutti/ml-canvas.git"
  },
  "bugs": {
    "url": "https://github.com/m-mutti/ml-canvas/issues"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode=lib",
    "build:demo": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepare": "npm run build"
  },
  "dependencies": {
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vitest/eslint-plugin": "^1.3.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "prettier": "3.6.2",
    "semantic-release": "^24.2.7",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^3.2.4"
  }
}
